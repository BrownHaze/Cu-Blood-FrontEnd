{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.18.0",
    "next": "^7.0.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3",
    "react-truncate": "^2.4.0",
    "@babel/core": "^7.0.0-beta",
    "babel-core": "^7.0.0-beta",
    "babel-runtime": "^7.0.0-beta"
  },
  "scripts": {
    "next:dev": "next",
    "next:build": "postcss ./components/core/tw.css -o ./components/core/tailwind.css | next build",
    "next:start": "next start",
    "start:tw": "postcss ./components/core/tw.css -o ./components/core/tailwind.css -w",
    "start": "next start -p 8000",
    "build": "react-scripts build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "eject": "react-scripts eject",
    "go": "concurrently --kill-others \"yarn start:tw\" \"yarn next:dev\"",
    "export": "yarn next:build | next export"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-plugin-root-import": "^6.1.0",
    "concurrently": "^4.1.0",
    "cssnano": "^4.1.8",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "postcss-cli": "^6.1.1",
    "postcss-easy-import": "^3.0.0",
    "tailwindcss": "^0.7.3"
  }
}
